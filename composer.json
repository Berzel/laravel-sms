{
    "name": "berzel/laravel-sms",
    "description": "Expressive and fluent unified API for sending SMS with laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Berzel Best",
            "email": "berzel.best@gmail.com"
        }
    ],
    "require": {
        "twilio/sdk": "^6.16",
        "illuminate/support": "^8.25"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Berzel\\LaravelSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Berzel\\LaravelSms\\ServiceProvider"
            ],
            "aliases": {
                "Sms" : "Berzel\\LaravelSms\\Sms"
            }
        }
    }
}
